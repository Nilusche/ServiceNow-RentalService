        <template><![CDATA[<div class="container">
  <div class="card" ng-repeat="item in data.items">
    <div class="card__header">
      <img ng-if="item.image!='' && item.rented == 0" src="{{item.image}}" alt="card__image" class="card__image" width="600" height="200">
      <img ng-if="item.image!='' && item.rented != 0" src="{{item.image}}" alt="card__image" class="card__image" width="600" height="200" class="rented">
      <img ng-if="item.image==''" src="data:image/png;base64," alt="card__image" class="card__image" width="600" height="200">
    
    </div>
    <div class="card__body">
      <span>
         <span class="tag tag-blue">{{item.category}}</span>
        <span ng-if="item.rented!=0" class="tag tag-red">Rented</span>
      </span>
     
      
      <h4>{{item.name}}</h4>
      <h3>{{item.price}}€</h3>
    </div>
    <div class="card__footer">
      <div class="user">
        <img src="{{item.avatar}}" class="user__image" width="35">
        <div class="user__info">
          <h5>{{item.ownername}}</h5>
          <small>{{item.created}}</small>
        </div>
      </div>
    </div>
  </div>
</div>]]></template>
